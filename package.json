		"graphql-modules": "^1.4.2",
		"graphql-tag": "2.12.4",
		"prettier": "2.3.0",
		"prettier-plugin-svelte": "2.3.0",
		"svelte": "3.38.2",
		"svelte-preprocess": "4.7.3",
		"tslib": "2.2.0",
		"typescript": "4.2.4",
		"vite": "2.3.3"
	},
	"dependencies": {
		"@urql/core": "2.1.2",
		"@urql/exchange-request-policy": "0.1.3",
		"@urql/svelte": "1.2.2",
		"graphql": "15.5.0"
